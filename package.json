{
  "name": "@gioacostax/create-react-kit",
  "version": "1.2.0",
  "description": "Create React App",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "test": "node index.js app --default && rm -rf app"
  },
  "author": "Giorgio Acosta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com:gioacostax/react-kit.git"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js"
  ],
  "bin": {
    "create-react-kit": "./index.js"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "cross-spawn": "^7.0.3",
    "inquirer": "^7.3.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "ignorePatterns": [
      "**/node_modules"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": 0,
      "lines-between-class-members": 0,
      "no-nested-ternary": 0,
      "object-curly-newline": [
        "warn",
        {
          "ObjectPattern": {
            "multiline": true,
            "minProperties": 5
          }
        }
      ],
      "prefer-arrow-callback": 0
    }
  }
}
